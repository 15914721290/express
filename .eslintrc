{
  "extends": "airbnb",
  "rules": {
    "strict": 0,
    "require-yield": 0,
    "no-param-reassign": 0,
    "import/no-extraneous-dependencies": 0,
    "no-use-before-define": 0,
    "consistent-return": 0,
    "no-unused-expressions": 0,
    "no-return-assign": 0,
    "func-names": ["error", "as-needed"],
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0,
    "prefer-spread": 0,
    "prefer-rest-params": 0,
    "arrow-parens": 0,
    "import/prefer-default-export": 0
  },
  "env": {
    "browser": "true",
    "node": "true"
  },
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true,
      "experimentalObjectRestSpread": true
    },
    "codeFrame": false
  },
  "parser": "babel-eslint"
}
